{
  "name": "api",
  "version": "0.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch . --ext ts --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "ts:typeorm": "cross-env NODE_ENV=development ts-node -r dotenv-flow/config ../../node_modules/typeorm/cli.js",
    "ts:migrate:create": "yarn ts:typeorm migration:create -d src/migrations/ -f src/ormconfig.ts -n",
    "ts:migrate:up": "yarn ts:typeorm migration:run -f src/ormconfig.ts",
    "ts:migrate:down": "yarn ts:typeorm migration:revert -f src/ormconfig.ts",
    "js:typeorm": "typeorm",
    "js:typeorm:dev": "cross-env NODE_ENV=development yarn js:typeorm",
    "js:typeorm:prod": "cross-env NODE_ENV=production yarn js:typeorm",
    "js:migrate:up:dev": "yarn js:typeorm:dev migration:run -f dist/ormconfig.js",
    "js:migrate:up:prod": "yarn js:typeorm:prod migration:run -f dist/ormconfig.js",
    "js:migrate:down:dev": "yarn js:typeorm:dev migration:revert -f dist/ormconfig.js",
    "js:migrate:down:prod": "yarn js:typeorm:prod migration:revert -f dist/ormconfig.js",
    "lint": "eslint src/**"
  },
  "dependencies": {
    "apollo-server-express": "^3.5.0",
    "argon2": "^0.28.2",
    "class-validator": "^0.14.0",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "ioredis": "^4.28.0",
    "jsonwebtoken": "^8.5.1",
    "ms": "^2.1.3",
    "mysql2": "^2.3.3",
    "nanoid": "^3.1.30",
    "nodemailer": "^6.7.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.17",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express-session": "^1.17.4",
    "@types/ioredis": "^4.28.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.2",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
