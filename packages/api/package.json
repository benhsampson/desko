{
  "name": "api",
  "version": "0.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch . --ext ts --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "_typeorm": "ts-node ../../node_modules/typeorm/cli.js",
    "migrate:create": "yarn _typeorm migration:create -d src/migrations/ -f src/ormconfig.ts -n",
    "migrate:up": "yarn _typeorm migration:run -f src/ormconfig.ts",
    "migrate:down": "yarn _typeorm migration:revert -f src/ormconfig.ts",
    "lint": "eslint src/**"
  },
  "dependencies": {
    "apollo-server-express": "^3.5.0",
    "argon2": "^0.28.2",
    "class-validator": "^0.13.1",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "ioredis": "^4.28.0",
    "jsonwebtoken": "^8.5.1",
    "ms": "^2.1.3",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.30",
    "nodemailer": "^6.7.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.17",
    "@types/express-session": "^1.17.4",
    "@types/ioredis": "^4.28.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/ms": "^0.7.31",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.3",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.2"
  }
}
